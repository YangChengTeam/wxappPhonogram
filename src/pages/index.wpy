   font-size: 18rpx;
   text-align: right;
}

.key-video {
    width: 578rpx;
    height: 316rpx;
    left: 75rpx;
    top: 230rpx;
    position: absolute;
}

.video {
  width: 602rpx;
  height: 340rpx;
  flex-direction: row;
  background: url('http://tic.upkao.com/Public/wxappImgs/video-bg.png') no-repeat;
  background-size: 100% 100%;
  position: relative;
}

.video-cover , .video-play {
    width: 578rpx;
    height: 316rpx;
    margin: 13rpx;
    position: absolute;
